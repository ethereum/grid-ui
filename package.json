{
  "name": "mist-ui-react",
  "version": "0.1.19",
  "private": true,
  "main": "./app/shell-electron.js",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereum/mist-ui-react.git"
  },
  "scripts": {
    "electron:dev": "cross-env PORT=3080 electron .",
    "tau:dev": "tau .",
    "start-conflict": "cross-env react-scripts start",
    "analyze-modules": "source-map-explorer build/static/js/1.*",
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "cross-env PORT=3080 CI='false' react-scripts start",
    "_build": "react-scripts build",
    "build": "react-scripts-ext build app",
    "deploy": "react-scripts-ext deploy",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "less-watch-compiler --run-once",
    "watch-css": "npm run build-css && less-watch-compiler",
    "lint:watch": "esw -w src --ext .jsx --ext .js",
    "lint": "eslint src --ext .jsx --ext .js"
  },
  "dependencies": {
    "bignumber.js": "^7.2.1",
    "bn.js": "^4.11.8",
    "dapp-styles": "git+https://github.com/ethereum/dapp-styles.git",
    "ethereum-react-components": "^1.11.2",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-md-spinner": "^0.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "semver": "^5.5.0",
    "web3": "^1.0.0-beta.36"
  },
  "ignore": {},
  "devDependencies": {
    "babel-register": "^6.26.0",
    "colors": "^1.3.0",
    "create-react-app-extensions": "^1.0.1",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "electron": "^2.0.8",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-watch": "^4.0.2",
    "husky": "^1.3.1",
    "less-watch-compiler": "^1.11.3",
    "lint-staged": "^8.1.1",
    "prettier": "^1.16.1",
    "react-test-renderer": "^16.5.2",
    "source-map-explorer": "^1.6.0",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --no-semi --single-quote --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
